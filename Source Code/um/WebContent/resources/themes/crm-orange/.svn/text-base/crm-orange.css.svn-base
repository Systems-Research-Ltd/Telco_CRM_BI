table.forms input, textarea{ height:35px; line-height:35px; padding:0px 0px 0px 10px; margin-bottom:10px; display:block; }
table.forms select { padding:9px;  }
table.forms .select2 { height:auto; line-height:35px !important; padding:8px; border: 1px solid #AAA; }
table.forms .submit { height:30px; border: 1px solid #AAA; padding: 4px; text-decoration: none; cursor: pointer; }
table.forms .browse-upload { height:30px; line-height:30px; border: 0px  none !important; padding: 0px; text-decoration: none;  }
