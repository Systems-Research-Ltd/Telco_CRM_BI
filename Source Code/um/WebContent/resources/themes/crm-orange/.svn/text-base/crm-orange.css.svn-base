@charset "utf-8";
/* CSS Document */

body, div, h1, h2, h3, h4, h5, h6, p, table, ol, ul, li{ padding:0px; margin:0px; }

body { background:url(themes/crm-orange/images/orange-black-bg.png.jsf) #ededed repeat-x 0 0; font-family:Arial, Helvetica, sans-serif; font-size:12px; color:#2a2926; }
.body-ip { background:none; }
a { text-decoration:none; color:inherit; }

h1, h2, h3, h4, h5, h6 { font-family:Arial, Helvetica, sans-serif; text-decoration:none; color:#2a2926; line-height:30px; }
h1 { font-size:25px; font-weight:normal; color:#6ba3e2; }
h2 { font-size:23px; font-weight:normal; color:#2a2926; }
h3 { font-size:20px; font-weight:normal; color:#ffa800; }
h4 { font-size:17px; font-weight:normal; color:#268f00; }
h5 { font-size:14px; font-weight:normal; }
h6 { font-size:12px; font-weight:normal; }
.login-screen { background:url(images/login-screen-bg.png) repeat-x 0 0; font-family:Arial, Helvetica, sans-serif; font-size:12px; color:#2a2926; }
.login-screen-border { width:998px; height:auto; margin:20px auto; padding:10px 0px 20px 0px; display:block; color:#9f9f9f; text-align:right; border-top:1px solid #CCC; }

/*Login screen */
.login-holder { width:480px; text-align:center; display:block; margin:0px auto; padding:80px 0px;}
.login-tbg { width:442px; height:10px; background:url(images/login-tbg.png) no-repeat center bottom; }
.login-bg { width:442px; height:auto; background:url(images/login-bg.png) repeat-y center top; }
.login-bbg { width:442px; height:10px; background:url(images/login-bbg.png) no-repeat center top; }
.heading-black { font:normal 21px/30px Arial, Helvetica, sans-serif; color:#2a2926; text-align:left; }
.heading-blue { font:bold 21px/50px Arial, Helvetica, sans-serif; color:#6ba3e2; text-align:left; }
.login { color:#2A2926; font-family:Arial, Helvetica, sans-serif; font-size:12px; }
.login a{ color:#6ba3e2; text-decoration:underline; }
.login input { width:343px; height:25px; line-height:25px; padding:3px; border:1px solid #CCC; }
input.login-btn  { width:72px; height:29px; background:url(images/login-btn.png) no-repeat 0 0; border:none 0px; }
/* form table  */
.form-table { width:100%; border: 0px;}
.form-required { color:#f00;}
.hidden {display:none !important;}
/* an attempt to over-write primefaces styles */
.ui-form textarea, .ui-form input:not([type="submit"]):not([type="button"]):not([type="checkbox"]):not([type="radio"]):not([type="file"]):not([type="range"]) {
	width: 97% !important;
	margin: 3px 0px;
	min-height: 24px;
}
.ui-form label {
	width: 100%;
	display: block;
	font-weight: bold;
	border-bottom: 1px solid #a8a8a8;
}
.prime-drop-down {
	width: 232px;
}

.ui-datatable .ui-datatable-data td, .ui-datatable .ui-datatable-data-empty td {
	border-width:0px !important;
}

.ui-widget-content {
border: 0px solid #a8a8a8 !important;
}

.ui-panelgrid td {
border-width: 0px !important;
padding-right: 0px;
}

.ui-fieldset {
	border: 1px solid #a8a8a8 !important;
	margin-bottom: 10px;
}

.ui-fieldset-legend {
	background: none !important;
	border: 0px !important;
	margin-left: 30px;
}

.bread-crumb {
padding-left: 10px !important;
}

/*////////////////////////////
Dashboard
/////////////////////////////*/
.ui-dashboard-column {width:50% !important;}
.flt-lft { float:left; }
.flt-ryt { float:right; }

.success-message { width:auto; height:40px; line-height:40px; padding:0px 10px 0px 40px; margin:0px auto; border:1px solid #c4df9b; font-weight:bold; color:#406618; display:block; background:url(icons/success-message.gif) 10px 10px no-repeat #ebfad5; border-radius: 5px 5px 5px 5px; }

.warning-message { width:auto; height:40px; line-height:40px; padding:0px 10px 0px 40px; margin:0px auto; border:1px solid #eaa900; font-weight:bold; color:#eaa900; display:block; background:url(icons/warning-message.png.jsf) 10px 10px no-repeat #fbf5e1; border-radius: 5px 5px 5px 5px;}

.error-message { height:40px; line-height:40px; padding:0px 10px 0px 40px; margin:0px auto; border:1px solid #F00; font-weight:bold; color:#F00; display:block; background:url(icons/error-message.gif) 10px 10px no-repeat #ffd4c7; border-radius: 5px 5px 5px 5px;}

.info-message { height:40px; line-height:40px; padding:0px 10px 0px 40px; margin:0px auto; border:1px solid #00aeef; font-weight:bold; color:#00aeef; display:block; background:url(icons/info-message.png.jsf) 10px 10px no-repeat #cfedf8; border-radius: 5px 5px 5px 5px;}

.saved-message { height:40px; line-height:40px; padding:0px 10px 0px 40px; margin:0px auto; border:1px solid #acacac; font-weight:bold; color:#252525; display:block; background:url(icons/saved-message.png.jsf) 10px 10px no-repeat #ebebeb; border-radius: 5px 5px 5px 5px;}

.clear { clear:both; }

table, td, tr { padding:0px; margin:0px; border-collapse:collapse; }

:focus {
	outline: 0;
}
        
/* rounder corner hover*/


/* default header */
#header { width:998px; height:91px; margin:0px auto; display:block; }
#header .logo{ display:block; float:left; padding-top:30px; border:0px none; }
#header .login-welcome{ display:block; float:right; padding-top:23px; color:#FFF; }
#header .login-welcome ul{ width:290px; padding:0px; margin:0px; float:left; display:block;}
#header .login-welcome li{ height:auto; line-height:27px; list-style-type:none; color:#888888; float:left; display:block; text-align:center;}
#header .login-welcome li a{ list-style-type:none; color:#888888; text-decoration:none; }
#header .sep { padding:0px 13px; display:block; text-align:center !important; }
#header .settings-icon { background:url(icons/settings-icon.png.jsf) no-repeat center 15px;}

/* orange menu */

.menu { width:998px; height:auto; margin:0px auto; display:block; }
.menu ul{ width:998px; height:auto; padding:0px; margin:0px; display:block; }
.menu li{ height:47px; line-height:47px; list-style-type:none; display:block; float:left; text-align:center; font-size:16px; font-weight:bold; color:#FFF; }
.menu li a{ display:block; padding:0px 22px;list-style-type:none; color:#FFF; text-decoration:none; }
.menu li a:hover{ display:block; color:#f9debe; text-decoration:none; background:url(themes/crm-orange/images/hover-orange.png.jsf) repeat-x 0 0; }
.menu li.selected{ display:block; color:#f9debe; text-decoration:none; background:url(themes/crm-orange/images/hover-orange.png.jsf) repeat-x 0 0; }
.menu li.sep { width:2px; height:47px; display:block; background:url(themes/crm-orange/images/default-menu-sep.png.jsf) no-repeat center top; }

/* orange sub-menu */
.sub-menu { width:998px; height:49px; margin:0px auto; display:block; }
.sub-menu ul{ width:998px; height:auto; padding:0px; margin:0px; display:block; }
.sub-menu li{ height:49px; line-height:49px; list-style-type:none; display:block; float:left; text-align:center; font-size:12px; font-weight:normal; color:#FFF; }
.sub-menu li a{ display:block; padding:0px 22px; list-style-type:none; color:#FFF; text-decoration:none; }
.sub-menu li a:hover{ display:block; color:#7a310a; text-decoration:none; background:url(themes/crm-orange/images/sub-menu-hover-orange.png.jsf) repeat-x 0 0; }
.sub-menu li.selected a{ display:block; color:#FFF; /*font-weight:bold;*/ text-decoration:none; background:url(themes/crm-orange/images/sub-menu-hover-orange.png.jsf) repeat-x 0 0; }
.sub-menu li.sep { width:1px; height:49px; display:block; background:url(themes/crm-orange/images/orange-sub-menu-sep.png.jsf) no-repeat center top; }

/* content area */
.content-area { width:998px; height:auto; margin:0px auto; padding:20px 0px 0px 0px; display:block; }
table.content-table{ width:100%; line-height:30px; border-collapse:collapse; }
table.content-table th { font:bold 14px/30px Arial, Helvetica, sans-serif; text-transform:uppercase; color:#FFF; background:#2A2926; padding:2px 2px 2px 7px;}
table.content-table tr{ width:auto; line-height:30px; }
/* table.content-table tr:hover{ background:#ededed; color:#2a2926; } */
/*table.content-table th:hover{ background:#569AE4; }*/
table.content-table td{ width:inherit; padding-right:0px; line-height:30px; vertical-align: bottom;}
table.content-table a{ color:#f17a23; text-decoration:none; font-weight:bold; }
table.content-table a:hover{ color:#2A2926; text-decoration:none; font-weight:bold; }

/* Dashboard table */
table.dash-table{ width:100%; border-collapse:collapse; }
table.dash-table th { text-transform:uppercase; color:#FFF; background:#2A2926;}
table.dash-table tr{ width:auto;}
table.dash-table td{ width:inherit; padding-right:30px; border-bottom:1px dotted #CCC; }
table.dash-table a{ color:#f17a23; text-decoration:none; font-weight:bold; }
table.dash-table a:hover{ color:#2A2926; text-decoration:none; font-weight:bold; }

/* tables with borders */
table.table-border{ border-collapse:collapse; margin:0px auto; }
table.table-border td{ padding:5px; border:1px solid #CCC; }
table.table-border a{ color:#09F; text-decoration:none; }

/* tables without borders */
table.table-noborder{ border-collapse:collapse; margin:0px auto; }
table.table-noborder tr, table.table-noborder td{ line-height:35px; border:0px none; }
table.table-noborder th{ line-height:30px; border:0px none; background:#333; color:#FFF; font:bold 14px/30px Arial, Helvetica, sans-serif; padding-left:10px; text-transform:uppercase; }
table.table-noborder a{ color:#09F; text-decoration:none; }


/* inputs overWrite*/
input[type="text"], input[type="password"], input[type="select"], input[type="search"], input[type="file"], input[type="date"], textarea, select {
    border:1px solid #C4C4C4
    color: #777777;
    padding: 4px;
}

input[type="text"].noborder-txtfield{
 	border:0px;
    outline: 0px;
    background: none repeat scroll 0 0 #FFFFFF;
    box-shadow: 0 0px 0px #FFFFFF inset;
    color: #313131;
}
input, select {
    vertical-align: middle;
}

