/* actions */

.actions { width:auto; display:block; padding:0px; margin:0px auto; }
.actions ul{ width:auto; display:block; padding:0px; margin:0px auto; float:left; }
.actions li{ display:inline; float:left; padding:0px 15px 0px 0px; margin:0px; list-style-type:none; }
.actions img { margin:0px auto; text-align:center; vertical-align:middle; border:0px none; }


/* search */
.search { width:338px; height:auto; display:block; margin:0px auto; padding:0px; overflow:hidden; }
.search input{ width:298px; height:30px; line-height:30px; padding:0px; border:0px none; background:url(themes/crm-orange/images/search-bg.png.jsf) repeat-x 0 0; color:#9f9f9f; }
.search-lc { width:10px; height:30px; background:url(themes/crm-orange/images/search-lc.png.jsf) no-repeat 0 0; }
.search-bg { width:298px; height:30px; background:url(themes/crm-orange/images/search-bg.png.jsf) repeat-x 0 0;  }
input.search-btn { width:30px; height:30px; background:url(themes/crm-orange/images/search-btn.png.jsf) no-repeat 0 0; }

/* search bar */
.search-bar-lc { width:7px; height:28px; background:url(themes/crm-orange/images/search-bar-lc.png.jsf) no-repeat top right;}
.search-bar-keyword{ color:#9f9f9f; }
input.search-bar-keyword{ float:left; color:#9f9f9f; margin-top:3px;  border:1px solid #000; }
.search-bar-btn { width:27px; height:28px; background:url(themes/crm-orange/images/search-bar-btn.png.jsf) no-repeat top left; border:0px none;}
input.search-bar-btn { width:26px; height:26px; display:block; background:url(themes/crm-orange/images/search-bar-btn.png.jsf) no-repeat 0 0; border:0px none; float:left; margin-top:3px; border:0px none; }

/* add-new-task */
.new-task { width:auto !important; display:block; padding-top:3px; font:normal 12px/30px Arial, Helvetica, sans-serif; color:#898989; text-transform:capitalize; float:right; }
.new-task ul { width:100%; float:left;}
.new-task li { float:left; list-style-type:none; }
.new-task li a { color:#898989; text-decoration:none; }
.new-task li a:hover{ color:#FFF; text-decoration:underline; }
.new-task li.sep { width:20px; display:block; text-align:center; }

/* 'CREATE NEW' button */
.create-btn-rc { width:10px; height:30px; float:left; display:block; background:url(themes/crm-orange/images/create-btn-rc.png.jsf) no-repeat left top; }
.create-btn-lc { width:10px; height:30px; float:left; display:block; background:url(themes/crm-orange/images/create-btn-lc.png.jsf) no-repeat right top; }
.create-btn-bg { width:auto; height:30px; line-height:30px; float:left; display:block; background:url(themes/crm-orange/images/create-btn-bg.png.jsf) repeat-x left top; }
span.create-btn-bg a { color:#c4c4c4; text-decoration:none; }
span.create-btn-bg a:hover { color:#fff; text-decoration:none; }
/* left menu */
.left-menu-widget { width:190px; height:auto; display:block; float:left; padding:0px 20px 20px 0px; margin:0px; }
/*left menu */
.lft-menu-trc{ width:10px; height:30px; background:url(themes/crm-orange/images/lft-menu-trc.png.jsf) no-repeat left top; }
.lft-menu-tbg{ width:170px; height:30px; line-height:30px; background:url(themes/crm-orange/images/lft-menu-tbg.png.jsf) repeat-x left top; color:#FFF; font:bold 14px/30px Arial, Helvetica, sans-serif; }
.lft-menu-tlc{ width:10px; height:30px; background:url(themes/crm-orange/images/lft-menu-tlc.png.jsf) no-repeat right top; }
.lft-menu-bg{ height:auto; background:url(themes/crm-orange/images/lft-menu-bg.jpg.jsf) repeat-y 0 0; }

.lft-menu-brc{ width:10px; height:30px; background:url(themes/crm-orange/images/lft-menu-brc.png.jsf) no-repeat left top; }
.lft-menu-bbg{ width:10px; height:10px; background:url(themes/crm-orange/images/lft-menu-bbg.png.jsf) repeat-x left top; }
.lft-menu-blc{ width:10px; height:10px; background:url(themes/crm-orange/images/lft-menu-blc.png.jsf) no-repeat right top; }

.left-menu { width:190px; height:auto; padding:0px; margin:0px auto; display:block; }
.left-menu ul { padding:0px; margin:0xp; }
.left-menu li { width:150px; height:30px; line-height:30px; float:left; padding:0px 20px; margin:0xp; border-bottom:1px solid #CCC; display:block; list-style-type:none; background:url(themes/crm-orange/images/bullet.png.jsf) left center no-repeat; }
.left-menu li a{ color:#2a2926; display:bloack; line-height:30px; text-decoration:none;  }
.left-menu li.selected a{ color:#2a7ad2; display:bloack; line-height:30px; text-decoration:none; font-weight:bold;  }

/* left tabs */
.left-tabs { width:190px; height:auto; padding:0px; margin:0px auto; display:block; }
.left-tabs ul { padding:0px; margin:0xp; }
.left-tabs li { width:140px; height:40px; line-height:40px; float:left; padding:0px 20px 0px 30px; margin:0xp; border-bottom:1px solid #CCC; display:block; list-style-type:none; background:url(icons/db-tbl.png.jsf) no-repeat 10px 10px; }
.left-tabs li a{ color:#2a2926; display:bloack; line-height:40px; text-decoration:none;  }
.left-tabs li.selected a{ color:#2a7ad2; display:bloack; line-height:40px; text-decoration:none; font-weight:bold;  }


/* contents */
.contents { width:788px; height:auto; display:block; float:right; }

/* contents holder */

.blue-tbl-trc{ width:10px; height:30px; background:url(themes/crm-orange/images/blue-tbl-trc.png.jsf) no-repeat left top; }
.blue-tbl-tbg{ width:768px; height:30px; line-height:30px; background:url(themes/crm-orange/images/blue-tbl-tbg.png.jsf) repeat-x left top; color:#FFF; font:bold 14px/30px Arial, Helvetica, sans-serif; }
.blue-tbl-tlc{ width:10px; height:30px; background:url(themes/crm-orange/images/blue-tbl-tlc.png.jsf) no-repeat right top; }
.blue-tbl-bg{ height:auto; background:url(themes/crm-orange/images/blu-tbl-bg.png.jsf) repeat-y 0 0; }

.bread-crumb { font:bold 12px/30px Arial, Helvetica, sans-serif; color:#FFF; text-transform: capitalize; }
.bread-crumb a{ font:bold 14px/30px Arial, Helvetica, sans-serif; color:#FFF; text-decoration:underline; }


/* footer orange */
.footer { width:998px; height:auto; margin:0px auto; padding:0px 0px 20px 0px; display:block; color:#9f9f9f; text-align:right; } 
.footer a{ color:#f17a23; text-decoration:underline; }
.footer a:hover{ color:#2a2926; text-decoration:underline; }

/* mandatory fields */
.mandatory { color:#F00; text-align:left; }

/* popup window */
.popup-trc { width:10px; height:30px; background:url(themes/crm-orange/images/popup-trc.png.jsf) no-repeat left top; }
.popup-tbg { width:auto; height:30px; background:url(themes/crm-orange/images/popup-tbg.png.jsf) repeat-x left top; line-height:30px; color:#fdc689; }
.popup-tbg a{ color:#fdc689; text-decoration:none; font-size:16px; }
.popup-tlc { width:10px; height:30px; background:url(themes/crm-orange/images/popup-tlc.png.jsf) no-repeat right top; }
.popup-bg { background:#1f1f1f; color:#dcdcdc; padding:10px 0px;}
.popup-rc { width:10px; height:auto; background:url(themes/crm-orange/images/popup-rc.png.jsf) repeat-y left top; }
.popup-lc { width:10px; height:auto; background:url(themes/crm-orange/images/popup-lc.png.jsf) repeat-y right top; }
.popup-brc { width:10px; height:10; background:url(themes/crm-orange/images/popup-brc.png.jsf) no-repeat left top; }
.popup-bbg { width:auto; height:10; background:url(themes/crm-orange/images/popup-bbg.png.jsf) repeat-x left top; }
.popup-blc { width:10px; height:10; background:url(themes/crm-orange/images/popup-blc.png.jsf) no-repeat right top; }
.yes-pad { padding-right:5px; }
.no-pad { padding-left:5px; }

/* pagination by Rizwan */

.tnt_pagination {
        display:block;
        text-align:right;
        height:22px;
        line-height:21px;
        clear:both;
        padding-top:3px;
        font-family:Arial, Helvetica, sans-serif;
        font-size:12px;
        font-weight:normal;
        padding-right:20px;
        padding-top:10px;
		background-color:#FFF;
}

.tnt_pagination a:link, .tnt_pagination a:visited{
        padding:7px;
        padding-top:2px;
        padding-bottom:2px;
        border:1px solid #EBEBEB;
        margin-left:10px;
        text-decoration:none;
        background-color:#F5F5F5;
        color:#6BA3E2;
        width:22px;
        font-weight:normal;
}

.tnt_pagination a:hover {
        background-color:#DDEEFF;
        border:1px solid #BBDDFF;
        color:#6BA3E2;  
}

.tnt_pagination .active_tnt_link {
        padding:7px;
        padding-top:2px;
        padding-bottom:2px;
        border:1px solid #BBDDFF;
        margin-left:10px;
        text-decoration:none;
        background-color:#DDEEFF;
        color:#6BA3E2;
        cursor:default;
}

.tnt_pagination .disabled_tnt_pagination {
        padding:7px;
        padding-top:2px;
        padding-bottom:2px;
        border:1px solid #EBEBEB;
        margin-left:10px;
        text-decoration:none;
        background-color:#F5F5F5;
        color:#D7D7D7;
        cursor:default; 
}

/* BI-Dashboard ORANGE Box for Contents */
.blue-box-trc { width:10px; height:30px; background:url(themes/crm-orange/images/orange-box-trc.png.jsf) no-repeat left top; }
.blue-box-tbg { width:978px; height:30px; background:url(themes/crm-orange/images/orange-box-tbg.png.jsf) repeat-x center bottom; }
.blue-box-tlc { width:10; height:30px; background:url(themes/crm-orange/images/orange-box-tlc.png.jsf) no-repeat right top; }

.blue-box-rc { width:10px; height:auto; background:url(themes/crm-orange/images/blue-box-rc.png.jsf) repeat-y left top; }
.blue-box-lc { width:10px; height:auto; background:url(themes/crm-orange/images/blue-box-lc.png.jsf) repeat-y right top; }

.blue-box-brc { width:10px; height:10px; background:url(themes/crm-orange/images/blue-box-brc.png.jsf) no-repeat left top; }
.blue-box-bbg { width:10px; height:10px; background:url(themes/crm-orange/images/blue-box-bbg.png.jsf) repeat-x center top; }
.blue-box-blc { width:10px; height:10px; background:url(themes/crm-orange/images/blue-box-blc.png.jsf) no-repeat right top; }

/* welcome admin BI Dashboard */
.welcome-bi-admin { border-bottom:3px solid #6ba3e2; }
.bi-contents-table { border-bottom:1px dotted #999; height:35px; line-height:35px; vertical-align:middle; color:#898989; }

/* blue titles */
.blue-titles { color:#6ba3e2;}
.green-titles { color:#39b54a; }
.red-titles { color:#f26c4f; }

/* Hints */
.hints { width:459px; height:30px; display:block; margin:0px auto; padding:0px; }
.hints ul { width:459px; height:30px; display:block; margin:0px auto; padding:0px; }
.hints li{ list-style-type:square; color:#898989; display:block; float:left; padding:0px 15px 0px 0px; }

/* Gray text */
.bi-table table, .bi-table td { color:#898989; }

/* TH links */
.th-links { text-align:right; line-height:30px; color:#898989; text-decoration:none; padding-right:10px; }
.th-links a{ color:#898989; text-decoration:none; }

/* ROUNDED CORNER BUTTON */
.button {
        display: inline-block;
        line-height: 1;
        padding: 7px 10px;
        text-decoration: none;
        font-weight: bold;
        color: #fff;
        background-color: #39c;
